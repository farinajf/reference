<?xml version="1.0" encoding="UTF-8"?>
<sequence name="AmtGenericOutSequence" trace="disable" xmlns="http://ws.apache.org/ns/synapse">
    <filter regex="[45]\d{2}$" source="$axis2:HTTP_SC">
        <then>
            <sequence key="AmtegaGenericFaultSequence"/>
        </then>
        <else/>
    </filter>
    <!--  +++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++
    +                                    SEND RESPONSE                                    +
    +++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++ -->
    <filter description="Si existe AmtegaMessageID se establece como HTTP Header" xpath="$ctx:AmtegaMessageID">
        <then>
            <property expression="$ctx:AmtegaMessageID" name="AmtegaMessageID" scope="transport" type="STRING"/>
        </then>
        <else/>
    </filter>
    <send/>
</sequence>
